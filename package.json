  "description": "slider ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-slider",
    "slider"
  ],
  "homepage": "http://github.com/react-component/slider",
  "author": "sima.zhang1990@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/slider.git"
  },
  "bugs": {
